<%- model_class = ClinicHistory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:city) %>:</strong></dt>
  <dd><%= @clinic_history.city %></dd>
  <dt><strong><%= model_class.human_attribute_name(:department) %>:</strong></dt>
  <dd><%= @clinic_history.department %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @clinic_history.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:cove) %>:</strong></dt>
  <dd><%= @clinic_history.cove %></dd>
  <dt><strong><%= model_class.human_attribute_name(:mobiel_service) %>:</strong></dt>
  <dd><%= @clinic_history.mobiel_service %></dd>
  <dt><strong><%= model_class.human_attribute_name(:airport) %>:</strong></dt>
  <dd><%= @clinic_history.airport %></dd>
  <dt><strong><%= model_class.human_attribute_name(:type_service) %>:</strong></dt>
  <dd><%= @clinic_history.type_service %></dd>
  <dt><strong><%= model_class.human_attribute_name(:patient_id) %>:</strong></dt>
  <dd><%= @clinic_history.patient_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:origin) %>:</strong></dt>
  <dd><%= @clinic_history.origin %></dd>
  <dt><strong><%= model_class.human_attribute_name(:destination) %>:</strong></dt>
  <dd><%= @clinic_history.destination %></dd>
  <dt><strong><%= model_class.human_attribute_name(:company) %>:</strong></dt>
  <dd><%= @clinic_history.company %></dd>
  <dt><strong><%= model_class.human_attribute_name(:accompanist_name) %>:</strong></dt>
  <dd><%= @clinic_history.accompanist_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:relationship) %>:</strong></dt>
  <dd><%= @clinic_history.relationship %></dd>
  <dt><strong><%= model_class.human_attribute_name(:phone) %>:</strong></dt>
  <dd><%= @clinic_history.phone %></dd>
  <dt><strong><%= model_class.human_attribute_name(:reason_for_consultation) %>:</strong></dt>
  <dd><%= @clinic_history.reason_for_consultation %></dd>
  <dt><strong><%= model_class.human_attribute_name(:current_illness) %>:</strong></dt>
  <dd><%= @clinic_history.current_illness %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              clinic_histories_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_clinic_history_path(@clinic_history), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              clinic_history_path(@clinic_history),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
